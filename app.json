{
  "expo": {
    "name": "ishikawa",
    "slug": "ishikawa",
    "version": "1.0.7",
    "orientation": "default",
    "icon": "./assets/IKG_ICON.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "plugins": [
      [
        "expo-image-picker",
        {
          "photosPermission": "写真を添付するためにフォトライブラリへのアクセスが必要です。"
        }
      ],
      "expo-secure-store"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.inoue420.ishikawa",
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false,
        "NSPhotoLibraryUsageDescription": "写真を添付するためにフォトライブラリへのアクセスが必要です。",
        "NSPhotoLibraryAddUsageDescription": "コメントに写真を添付・保存するために使用します。"
      },
      "icon": "./assets/IKG_ICON.png"
    },
    "android": {
      "edgeToEdgeEnabled": true,
      "package": "com.inoue420.ishikawa",
      "adaptiveIcon": {
        "foregroundImage": "./assets/IKG_adaptive_foreground.png",
        "backgroundColor": "#000000",
        "monochromeImage": "./assets/IKG_adaptive_mono.png"
      }
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "extra": {
      "eas": {
        "projectId": "41caced6-d569-45ba-b05d-c7572b112645"
      }
    }
  }
}
